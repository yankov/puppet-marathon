{
  "name": "potto-marathon",
  "version": "0.6.0",
  "author": "Paul Otto",
  "summary": "Module managing Marathon framework for Mesos",
  "license": "Apache-2.0",
  "source": "http://github.com/potto007/puppet-marathon",
  "project_page": "https://github.com/potto007/puppet-marathon",
  "issues_url": "https://github.com/potto007/puppet-marathon/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.1.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "deric/mesos",
      "version_requirement": ">= 0.6.2"
    }
  ],
  "requirements": [
  	{
  		"name": "puppet",
  		"version_requirement": ">=3.5.1 < 4.0.0"
  	}
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "14.04"
      ]
    }
  ]
}

